#include <iostream>

int main()
{
    printf("문장"); //문장이라는 문자를 출력
    printf("Hello World");
    printf("Hello\nWorld"); //줄 구분해서 출력
    printf("\"hello\""); //\"사용시 "같이 출력
    printf("\\hello world\\"); // \\사용시 \를 같이 출력
//d // 10진수로 표현해보자
    int n = 10; //정수를 입력
    printf("%d",n);
    //실수형
    double d = 12.345;
    printf("%f", d); //f d 같은거 겹치지 않게 조심하기
    //지수형
    double z = 12.3456789;
    printf("%e", z);
    //8진수로 출력
    int a = 1023;
    printf("%o", a);
    //16진수로 출력
    int a1 = 1023;
    printf("%x", a1);
    //부호없는 10진수로 출력
    int a2 = 1023;
    printf("%u", a2);
    //실수형으로 자동 출력
    double d1 = 12.3456789;
    printf("%g", d1);
    //하나의 문자로 출력
    char c = 'a';
    printf("%c", c);
    //문자열을 출력
    char x[5] = "abcd";
    printf("%s",x);

    return 0; //끝내는 명령어
}
